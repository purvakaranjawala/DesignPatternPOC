<div id="users">
  <table cellspacing="5" cellpadding="1" border="1">

    <thead class="thead-dark">
      <tr>
        <th>UserName</th>
        <th>Name</th>
        <th>Address</th>
        <th>City</th>
        <th>ContactNumber</th>
        <th>Created At</th>
        <th colspan="3">Actions</th>
      </tr>
    </thead>

    <tbody>
      <% users.each do |user| %>
        <tr>
          <td> <%= user.user_name  %> </td>
          <td> <%= user.full_name  %> </td>
          <td> <%= user.address %> </td>
          <td> <%= user.city %> </td>
          <td> <%= user.contact_number %> </td>
          <td> <%= user.created_at %> </td>
          <td> <%= link_to "show", user %></td>
        </tr>
      <% end %>
    </tbody>

  </table>
</div>
