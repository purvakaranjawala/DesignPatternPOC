<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.full_name %>
  </p>
  <p>
    <strong>Address:</strong>
    <%= user.address %>
  </p>

  <p>
    <strong>Landmark:</strong>
    <%= user.landmark %>
  </p>

  <p>
    <strong>City:</strong>
    <%= user.city %>
  </p>

  <p>
    <strong>Pincode:</strong>
    <%= user.pincode %>
  </p>

  <p>
    <strong>Contact number:</strong>
    <%= user.contact_number %>
  </p>

  <p>
    <strong>Created At:</strong>
    <%= user.created_at %>
  </p>

</div>
